{
  "compileOnSave": false,
  "compilerOptions": {
    "incremental": true,
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "study-core-ngx": [
        "dist/study-core-ngx"
      ],
      "study-core-ngx/*": [
        "dist/study-core-ngx/*"
      ],
      "study-ui-ngx": [
        "dist/study-ui-ngx"
      ],
      "study-ui-ngx/*": [
        "dist/study-ui-ngx/*"
      ]
    }
  }
}
